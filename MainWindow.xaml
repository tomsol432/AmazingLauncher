<Window x:Name="MWindow" x:Class="AmazingLauncher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AmazingLauncher"
        mc:Ignorable="d"
        Title="Amazing Launcher" Height="400" Width="400" ResizeMode="CanMinimize" FontFamily="VCR OSD Mono" FontSize="14">
    <Window.Background>
        <ImageBrush ImageSource="GFX/BCKGR.png"/>
    </Window.Background>
    <Grid>
        <Button Content="Run" HorizontalAlignment="Left" Width="199" Background="{x:Null}" BorderBrush="{x:Null}" Height="48" VerticalAlignment="Bottom" Margin="1,0,0,48" Foreground="#FF23FF00" FontSize="18" Click="Button_Click"/>
        <Button Content="Options" HorizontalAlignment="Left" Width="194" Background="{x:Null}" BorderBrush="{x:Null}" Height="48" VerticalAlignment="Bottom" Margin="1,0,0,0" Foreground="#FF23FF00" FontSize="18" Click="Button_Click_1"/>
        <MediaElement x:Name="Melement" Margin="0,0,10,10" RenderTransformOrigin="2.421,2.339" Height="177" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="227" Source="GFX/Darude_-_Sandstorm.mid" Loaded="Melement_Loaded" LoadedBehavior="Manual"/>

    </Grid>
</Window>
